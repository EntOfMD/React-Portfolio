{
    "name": "react-portfolio",
    "version": "0.1.0",
    "description": "react portfolio for ashell fernando 2019",
    "main": "server.js",
    "scripts": {
        "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
        "start:prod": "node server.js",
        "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
        "client": "cd client && npm run start",
        "install": "cd client && npm install",
        "build": "cd client && npm run build",
        "heroku-postbuild": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EntOfMD/React-Portfolio.git"
    },
    "author": "ashell f_ | entofmd",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/EntOfMD/React-Portfolio/issues"
    },
    "homepage": "https://github.com/EntOfMD/React-Portfolio#readme",
    "devDependencies": {
        "morgan": "^1.9.1"
    },
    "dependencies": {
        "concurrently": "^4.1.1",
        "axios": "^0.19.0",
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "helmet": "^3.20.0",
        "if-env": "^1.0.4",
        "mongoose": "^5.7.4"
    }
}
